<% provide(:title, 'All users') %>
<h1>Nearby Students</h1>
<ul class="users">
  <% @users.each do |user| %>
      <li>
        <%= gravatar_for user, size: 152 %>
        <%= link_to user.name, user %>
        <%= "--------Address:" %>
        <%= link_to user.address, user%>

      </li>
  <% end %>
</ul>